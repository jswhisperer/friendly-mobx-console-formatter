    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.12.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
